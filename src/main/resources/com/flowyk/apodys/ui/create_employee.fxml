<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="354.0"
            prefWidth="439.0"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.flowyk.apodys.ui.CreateEmployeeController">
    <GridPane layoutX="30.0" layoutY="28.0" prefHeight="254.0" prefWidth="404.0" AnchorPane.leftAnchor="10.0"
              AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
        <Label text="%name"/>
        <Label text="%email" GridPane.rowIndex="1"/>
        <TextField fx:id="meno" GridPane.columnIndex="1"/>
        <TextField fx:id="email" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </GridPane>
    <ButtonBar layoutX="176.0" layoutY="233.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
        <buttons>
            <Button defaultButton="true" onAction="#handleOk" text="%create_new_employee"/>
            <Button onAction="#handleCancel" prefWidth="120.0" text="%cancel"/>
        </buttons>
    </ButtonBar>
</AnchorPane>